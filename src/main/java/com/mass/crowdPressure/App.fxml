<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.canvas.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.paint.*?>
<?import javafx.scene.text.*?>

<VBox xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.mass.crowdPressure.gui.GUIController">
  <children>
    <MenuBar VBox.vgrow="NEVER">
      <menus>
        <Menu mnemonicParsing="false" text="File">
          <items>
            <MenuItem mnemonicParsing="false" text="New" />
            <MenuItem mnemonicParsing="false" text="Quit" />
          </items>
        </Menu>
        <Menu mnemonicParsing="false" text="Help">
          <items>
            <MenuItem mnemonicParsing="false" text="About" />
          </items>
        </Menu>
      </menus>
    </MenuBar>
      <HBox alignment="CENTER" spacing="10.0">
         <children>
            <Button fx:id="pauseStartButton" mnemonicParsing="false" onAction="#pauseStartSim" text="Start/Pause" />
            <Button mnemonicParsing="false" text="Next step &gt;" />
            <Region prefWidth="200.0" />
             <ComboBox fx:id="actionComboBox" onAction="#chooseAction" prefWidth="150.0" promptText="Action">
                 <opaqueInsets>
                     <Insets />
                 </opaqueInsets>
             </ComboBox>
            <VBox alignment="CENTER">
               <children>
                  <Label text="fps: ">
                     <VBox.margin>
                        <Insets bottom="5.0" />
                     </VBox.margin>
                  </Label>
                  <Slider fx:id="fpsSlider" blockIncrement="5.0" min="10.0" showTickLabels="true" showTickMarks="true" value="60.0" />
               </children>
               <HBox.margin>
                  <Insets left="10.0" />
               </HBox.margin>
            </VBox>
         </children>
         <VBox.margin>
            <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
         </VBox.margin>
      </HBox>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
         <children>
            <ScrollPane fx:id="scrollPane" hbarPolicy="ALWAYS" layoutX="204.0" layoutY="-46.0" prefHeight="317.0" prefWidth="640.0" vbarPolicy="ALWAYS" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
               <content>
                  <RadioButton mnemonicParsing="false" text="RadioButton" />
               </content></ScrollPane>
         </children>
    </AnchorPane>
      <HBox>
         <children>
            <Label text="Label" />
         </children>
         <VBox.margin>
            <Insets bottom="2.0" left="2.0" right="2.0" top="2.0" />
         </VBox.margin>
      </HBox>
  </children>
</VBox>
